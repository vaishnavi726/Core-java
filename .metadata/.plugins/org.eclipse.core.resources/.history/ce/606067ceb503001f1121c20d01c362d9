package com.event.test;

import com.eventmanagementsystem.Event;
import com.eventmanagementsystem.Orgainzer;

public class Demo {
	public static void main(String[] args) {
		
		Orgainzer organizer = new Orgainzer(45l,"singingevent");
		System.out.println(organizer.id +" " +organizer.name);
		
		Event event = new Event(67l, "gameshow", "All are welcome");
		System.out.println(event.id + " "+ event.name +" "+event.description);
		
		
	}

}
